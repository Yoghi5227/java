import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner (System.in);
        int a = sc.nextInt();
        for (int i = 1;i<=a;i++)
        System.out.print(i +" ");
        for(int i=a;i>=1;i--)
        System.out.print(i+" ");
        
             
        
    }
}
